<?xml version="1.0" encoding="UTF-8" ?>
<searchTemplate xmlns="http://rs.tdwg.org/tapir/1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://rs.tdwg.org/tapir/1.0 
    http://rs.tdwg.org/tapir/1.0/schema/tapir.xsd">
    <externalOutputModel
        location="http://rs.tdwg.org/ontology/voc/tapir/outputmodel/TaxonOccurrence/TaxonOccurrence.xml"/>
    <filter>
        <and>
            <greaterThan>
                <concept id="http://rs.tdwg.org/ontology/voc/TaxonOccurrence#/rdf:RDF/to:TaxonOccurrence/dcterm:modified"/>
                <parameter name="from"/>
            </greaterThan>
            <lessThan>
                <concept id="http://rs.tdwg.org/ontology/voc/TaxonOccurrence#/rdf:RDF/to:TaxonOccurrence/dcterm:modified"/>
                <parameter name="until"/>
            </lessThan>
        </and>
    </filter>
</searchTemplate>
