      <rdfs:comment> The class for constructing controlled vocabularies associated with Cyclisity</rdfs:comment>
      <rdfs:subClassOf rdf:resource="http://rs.tdwg.org/ontology/Base#DefinedTerm"/>
      <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/SpeciesProfileModel"/>
   </owl:Class>

   <!-- Circadian -->
   <CyclicityTerm rdf:ID="Circadian">
      <rdfs:label>Circadian</rdfs:label>
