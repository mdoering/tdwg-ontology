      <dc:publisher>TDWG Biodiversity Information Standards</dc:publisher>
      <dcterms:issued>2007-04-17</dcterms:issued>
      <dcterms:modified>$Date: 2007-08-15 14:05:50 +0100 (Wed, 15 Aug 2007) $</dcterms:modified>
      <dc:description>Ontology describing a controlled vocabulary for cyclicity.</dc:description>

      <owl:versionInfo>0.3</owl:versionInfo>

   </owl:Ontology>


   <!-- ************************************
               CyclicityTerm
 		************************************ -->

   <owl:Class rdf:ID="CyclicityTerm">
